# Google Cloud Configuration
GCP_PROJECT_ID=your-gcp-project-id
GCP_SERVICE_ACCOUNT_JSON=path/to/service-account.json

# AlloyDB Configuration (uses IAM authentication)
ALLOYDB_INSTANCE=your-alloydb-instance
ALLOYDB_CLUSTER=your-alloydb-cluster
ALLOYDB_REGION=your-region
ALLOYDB_DATABASE=your-database-name
# For IAM auth, use your service account email (e.g., my-sa@project-id.iam.gserviceaccount.com)
ALLOYDB_USER=your-service-account-email

# Vertex AI Configuration
VERTEX_AI_LOCATION=us-central1

# Application Configuration
RESULT_LIMIT=10
SESSION_MEMORY_SIZE=10
RESPONSE_TIMEOUT_SECONDS=10
MAX_RETRIES=3
